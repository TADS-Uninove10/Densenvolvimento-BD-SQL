CREATE TABLE FUNCIONARIO(
COD_FUNC NUMBER(7)CONSTRAINTS FUNC_PK PRIMARY KEY,
NOME_FUNC VARCHAR2(50),
ENDER_FUNC VARCHAR2(50),
CEP NUMBER(9),
FONE NUMBER(13),
COD_ESP NUMBER(7));

-----DROP TABLE FUNCIONARIO;

