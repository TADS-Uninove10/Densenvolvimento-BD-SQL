CREATE TABLE ESPECIALIDADES(
COD_ESP NUMBER(7)CONSTRAINTS ESP_PK PRIMARY KEY,
DESCRICAO_ESP VARCHAR2(50));

INSERT INTO ESPECIALIDADES VALUES
